{
  "expo": {
    "name": "Storytime For Kids",
    "slug": "storytime-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/new-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/new-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#fbf8f7"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/new-logo.png",
        "backgroundColor": "#fbf8f7"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/icons/new-logo.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
